{
    "productName": "chipbox",
    "version": "0.1.0",
    "identifier": "com.chipnertkj.chipbox",
    "build": {
        "beforeDevCommand": "trunk serve",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "trunk build",
        "frontendDist": "../dist"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [
            {
                "decorations": false,
                "fullscreen": false,
                "width": 800,
                "height": 600,
                "resizable": true,
                "title": "chipbox"
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all"
    }
}
